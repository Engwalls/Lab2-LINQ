@model IEnumerable<Lab2Test.Models.Lab2ViewModel>
@{
    ViewData["Title"] = "GetStudentsNTeachers";
}
<h1>
    This is every student in Programmering 1 and their teacher in that subject.
</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudFullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TeacherFullName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StudFullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CourseName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TeacherFullName)
                </td>
            </tr>
        }
    </tbody>
</table>


